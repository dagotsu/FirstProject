<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>BusinessAnalysis</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>Encoding</filter-name>
    <filter-class>jp.com.example.FilterService</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>Encoding</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>HelloServlet</servlet-name>
    <servlet-class>jp.com.example.HelloServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloServlet</servlet-name>
    <url-pattern>/HelloServlet/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RefPanel</servlet-name>
    <servlet-class>jp.ba.serv.RefPanel</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RefPanel</servlet-name>
    <url-pattern>/RefPanel/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DataPanel</servlet-name>
    <servlet-class>jp.ba.serv.DataPanel</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DataPanel</servlet-name>
    <url-pattern>/DataPanel/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>NouhinPanel</servlet-name>
    <servlet-class>jp.ba.serv.NouhinPanel</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>NouhinPanel</servlet-name>
    <url-pattern>/NouhinPanel/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>GISEngineOSPanel</servlet-name>
    <servlet-class>jp.ba.serv.GISEngineOSPanel</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GISEngineOSPanel</servlet-name>
    <url-pattern>/GISEngineOSPanel/*</url-pattern>
  </servlet-mapping>
</web-app>